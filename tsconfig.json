{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/abis/*": [
        "src/abis/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/connectors/*": [
        "src/connectors/*"
      ],
      "@/constants/*": [
        "src/constants/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/theme/*": [
        "src/theme/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/context/*": [
        "src/context/*"
      ],
      "@/reducer": [
        "src/reducer/index.ts"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/public/*": [
        "public/*"
      ]
    },
    "incremental": true
  },
  "typeRoots": ["./src/types"],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "graphql.d.ts"
, "lingui.config.js"  ],
  "exclude": [
    "node_modules",
    ".cache"
  ]
}
